		<meta http-equiv="x-ua-compatible" content="ie=11">
		<script>
			var shown = []
			function show(id) {
				var toShow = document.getElementById(id)
				if (!toShow) {return}
				shown.push(toShow)
				toShow.hidden = false
			}
			function hideAll() {
				for (var i = 0;i < shown.length;i++) {shown[i].hidden = true}
				shown = []
			}
			window.onload = function() {show('$index')}
		</script>
